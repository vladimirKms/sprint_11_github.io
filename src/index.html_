  <div class="root">
    <header class="header root__section">
      <img src="./images/logo.svg" alt="mesto logo" class="logo">
    </header>
    <div class="profile root__section">
      <div class="user-info">
        <div class="user-info__photo"></div>
        <div class="user-info__data">
          <h1 class="user-info__name">Jaques Causteau</h1>
          <p class="user-info__job">Sailor, Researcher</p>
          <button class="button user-about__button">Edit</button>
        </div>
        <!--button class="button user-info__button">+</button-->
        <button class="button placesAdd__button">+</button>
      </div>
    </div>
    <div class="places-list root__section">

    </div>
    <div class="popup">
      <div class="popup__content">
        <img src="./images/close.svg" alt="" class="popup__close">
        <h3 class="popup__title">Новое место</h3>
        <form class="popup__form" name="new">
            <input type="text" name="name" class="popup__input popup__input_type_name" minlength="2" maxlength="30" placeholder="Название"
            required>
            <h4 id="name-error" class="popup__input_message_imag-name" ></h4>
            <input type="url" name="link" class="popup__input popup__input_type_link-url" placeholder="Ссылка на картинку" required>
            <h4 id="link-error" class="popup__input_message_image-link"></h4>
            <button  class="button popup__button">+</button>
        </form>
      </div>
    </div> 
   <div class="popup popup__user">
    <div class="popup__content popup__content_user">
    <img src="./images/close.svg" alt="" class="popup__close popup__close_user">
    <h3 class="popup__title popup__title_user">Редактировать профиль</h3>
    <form class="popup__form popup__form_user" name="user">
        <input type="text" name="formNameUser" class="popup__input popup__input_type_name-user" minlength="2" maxlength="30"  placeholder="Имя" required>
        <h4 id="formNameUser-error" class="popup__form_user_message-name"></h4>
        <input type="text" name="formJobUser" class="popup__input popup__input_type_about-user"  minlength="2" maxlength="30" placeholder="О себе" required>
        <h4 id="formJobUser-error" class="popup__form_user_message-job"></h4>
        <button  class="button popup__button_submit popup__button_about-user ">Сохранить</button>
    </form>
    </div> 
  </div>


